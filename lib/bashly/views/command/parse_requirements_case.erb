# :command.parse_requirements_case
<%- if args.any? -%>
<%- condition = "if" -%>
<%- args.each do |arg| -%>
<%= condition %> [[ ! ${args[<%= arg.name %>]} ]]; then
  args[<%= arg.name %>]=$1
  shift
<%- condition = "elif" -%>
<%- end -%>
else
  printf "<%= strings[:invalid_argument] %>\n" "$key"
  exit 1
fi
<%- else -%>
printf "<%= strings[:invalid_argument] %>\n" "$key"
exit 1
<%- end -%>
