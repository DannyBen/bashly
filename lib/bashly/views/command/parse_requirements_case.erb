# :command.parse_requirements_case
% if args.any?
% condition = "if"
% args.each do |arg|
<%= condition %> [[ ! -v args[<%= arg.name %>] ]]; then
  args[<%= arg.name %>]=$1
  shift
% condition = "elif"
% end
else
% if catch_all
  other_args+=("$1")
  shift
% else
  printf "<%= strings[:invalid_argument] %>\n" "$key"
  exit 1
% end
fi
% elsif catch_all
  other_args+=("$1")
  shift
% else
printf "<%= strings[:invalid_argument] %>\n" "$key"
exit 1
% end
