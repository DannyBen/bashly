# :command.parse_requirements_while
while [[ $# -gt 0 ]]; do
  key="$1"
  case "$key" in
  <%- flags.each do |flag| -%>
<%= flag.render(:case).indent 2 %>

  <%- end -%>

  -* )
    printf "<%= strings[:invalid_flag] %>\n" "$key"
    exit 1
    ;;

  * )
<%= render(:parse_requirements_case).indent 4 %>
    ;;

  esac
done